<p id="notice"><%= notice %></p>

<h1>Tickets Users</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tickets_users.each do |tickets_user| %>
      <tr>
        <td><%= link_to 'Show', tickets_user %></td>
        <td><%= link_to 'Edit', edit_tickets_user_path(tickets_user) %></td>
        <td><%= link_to 'Destroy', tickets_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tickets User', new_tickets_user_path %>
